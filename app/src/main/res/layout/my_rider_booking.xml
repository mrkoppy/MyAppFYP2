<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="175dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="15dp"
    android:background="#ffdab3"
    android:clickable="true"
    android:focusable="true"
    android:theme="@style/NoActionBar">

    <RelativeLayout
        android:layout_width="190dp"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/tvFrom1"
            android:layout_width="45dp"
            android:layout_height="45dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Origin : "
                android:textSize="12sp"
                android:textColor="#000"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/from1"
            android:layout_width="155dp"
            android:layout_height="45dp"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/tvFrom1"
            android:layout_toEndOf="@+id/tvFrom1">

            <TextView
                android:id="@+id/tv_availableRideFrom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="0dp"
                android:text="@string/from"
                android:textColor="#000"
                android:textSize="12sp"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tvTo1"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_below="@+id/tvFrom1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="To : "
                android:textSize="12sp"
                android:textColor="#000"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/to1"
            android:layout_width="155dp"
            android:layout_height="45dp"
            android:layout_below="@+id/from1"
            android:layout_toEndOf="@+id/tvTo1"
            android:layout_toRightOf="@+id/tvTo1">

            <TextView
                android:id="@+id/tv_availableRideTo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:text="To : "
                android:textColor="#000"
                android:textSize="12sp"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tvDate1"
            android:layout_width="45dp"
            android:layout_height="30dp"
            android:layout_below="@+id/tvTo1">

            <TextView
                android:id="@+id/textdate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/date_rider_booking"
                android:textColor="#000"
                android:textSize="12sp"
                app:fontFamily="@font/cabin"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true"
                android:text="Time : "
                android:textColor="#000"
                android:textSize="12sp"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/date1"
            android:layout_width="155dp"
            android:layout_height="30dp"
            android:layout_below="@+id/to1"
            android:layout_toRightOf="@id/tvDate1">

            <TextView
                android:id="@+id/tv_availableRideDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/date"
                android:textColor="#000"
                android:textSize="13sp"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tvEST1"
            android:layout_width="55dp"
            android:layout_height="30dp"
            android:layout_below="@+id/tvDate1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Duration :"
                android:textSize="12sp"
                android:textColor="#000"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/EST1"
            android:layout_width="155dp"
            android:layout_height="30dp"
            android:layout_below="@+id/date1"
            android:layout_toRightOf="@+id/tvEST1">

            <TextView
                android:id="@+id/tv_availableRideEST"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/est"
                android:textColor="#000"
                android:textSize="12sp"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tvPrice1"
            android:layout_width="40dp"
            android:layout_height="25dp"
            android:layout_below="@+id/tvEST1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Price : "
                android:textSize="12sp"
                android:textColor="#000"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="155dp"
            android:layout_height="25dp"
            android:layout_below="@id/EST1"
            android:layout_toRightOf="@+id/tvPrice1">

            <TextView
                android:id="@+id/inRM"
                android:layout_width="25dp"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="RM "
                android:textColor="#000"
                android:gravity="center"
                app:fontFamily="@font/cabin"/>


            <TextView
                android:id="@+id/tv_availableRidePrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/inRM"
                android:text="Price : "
                android:textColor="#000"
                android:textSize="12sp"
                android:layout_toRightOf="@+id/inRM"
                app:fontFamily="@font/cabin"/>

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--Layout for top layout-->
        <RelativeLayout
            android:id="@+id/RLfortoplayout"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="center">

            <RelativeLayout
                android:id="@+id/left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_carplate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:text="Car Plate"
                    android:textColor="@android:color/transparent"
                    android:textSize="0sp" />

                <TextView
                    android:id="@+id/tv_carseat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:text="Car Seats"
                    android:textColor="@android:color/transparent"
                    android:textSize="0sp" />

                <TextView
                    android:id="@+id/tv_seatsleft"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:text="seats_left"
                    android:textColor="@android:color/transparent"
                    android:textSize="0sp" />
                <TextView
                    android:id="@+id/tv_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:text="gender"
                    android:textColor="@android:color/transparent"
                    android:textSize="0sp" />

                <TextView
                    android:id="@+id/tv_drivername"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:text="drivername"
                    android:textColor="@android:color/transparent"
                    android:textSize="0sp" />

                <Button
                    android:id="@+id/b1green"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="16dp"
                    android:background="#02FF02" />

                <TextView
                    android:id="@+id/tv_green"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/b1green"
                    android:gravity="center"
                    android:text="Available"
                    android:textSize="12sp"
                    app:fontFamily="@font/cabin" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/left">

                <Button
                    android:id="@+id/b2red"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_alignParentTop="true"
                    android:background="#ff0000"
                    android:gravity="center"/>

                <TextView
                    android:id="@+id/tv_red"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/b2red"
                    android:gravity="center"
                    android:text="Occupied"
                    android:textSize="12sp"
                    app:fontFamily="@font/cabin" />


            </RelativeLayout>

            <!-- -->

        </RelativeLayout>

        <!--Layout for bottom layout-->
        <RelativeLayout
            android:id="@+id/RLforBottomlayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/RLfortoplayout">

            <!--Layout for S1 -->
            <RelativeLayout
                android:id="@+id/RLforS1"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:gravity="center">

                <Button
                    android:id="@+id/ATButton1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:text="S1"
                    android:textSize="12sp" />

            </RelativeLayout>
            <!--Layout for S1 -->

            <!--Layout for S2 -->
            <RelativeLayout
                android:id="@+id/RLforS2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginLeft="1dp"
                android:layout_toRightOf="@+id/RLforS1"
                android:gravity="center">

                <Button
                    android:id="@+id/ATButton2"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true"
                    android:gravity="center"
                    android:text="S2"
                    android:textSize="12sp" />

            </RelativeLayout>
            <!--Layout for S2 -->

            <!--Layout for S3-->
            <RelativeLayout
                android:id="@+id/RLforS3"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_toRightOf="@+id/RLforS2"
                android:gravity="center">

                <Button
                    android:id="@+id/ATButton3"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true"
                    android:gravity="center"
                    android:text="S3"
                    android:textSize="12sp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RLforS4"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_below="@+id/RLforS1"
                android:gravity="center">

                <Button
                    android:id="@+id/ATButton4"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:text="S4"
                    android:gravity="center"
                    android:textSize="12sp"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RLforS5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_toRightOf="@+id/RLforS4"
                android:layout_below="@+id/RLforS2"
                android:gravity="center">

                <Button
                    android:id="@+id/ATButton5"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:text="S5"
                    android:gravity="center"
                    android:textSize="12sp"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RLforS6"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_toRightOf="@+id/RLforS5"
                android:layout_below="@+id/RLforS3"
                android:gravity="center">

                <Button
                    android:id="@+id/ATButton6"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:text="S6"
                    android:gravity="center"
                    android:textSize="12sp"/>

            </RelativeLayout>



        </RelativeLayout>



    </RelativeLayout>

</LinearLayout>